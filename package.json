{
  "name": "storm-dom",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel-node tools && npm run sass",
    "watch": "concurrently \"babel-node tools/dom/watch\" \"npm run js:watch\"",
    "start": "npm run build && concurrently \"npm run watch\" \"node-sass -w src/css -o build/static/css\" \"browser-sync start -s \"build\" -f \"build\" --no-notify\"",
    "serve": "browser-sync start -s \"build\" -f \"build\"",
    "test": "jest",
    "js": "browserify src/js/app.js -o build/static/js/app.js -t [ babelify ]",
    "js:watch": "babel-node tools/js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "sass": "node-sass src/css -o build/static/css"
  },
  "author": "stormid",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "chokidar": "^2.0.4",
    "concurrently": "^4.0.1",
    "fs-extra": "^7.0.1",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.10.0",
    "preact-render-to-string": "^4.1.0",
    "storm-storybook-markup": "^0.1.0"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^4.0.4",
    "@storybook/html": "^4.0.4",
    "storm-toggle": "^1.3.1"
  }
}
