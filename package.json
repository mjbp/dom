{
  "name": "storm-dom",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel-node tools && npm run sass",
    "gulp": "node_modules/.bin/gulp",
    "watch": "babel-node tools/watch",
    "start": "concurrently \"npm run watch\" \"node-sass -w src/css -o build/static/css\" \"browser-sync start -s \"build\" -f \"build\" --no-notify\"",
    "serve": "browser-sync start -s \"build\" -f \"build\"",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "sass": "node-sass src/css -o build/static/css"
  },
  "author": "stormid",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/runtime": "^7.1.5",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "chokidar": "^2.0.4",
    "concurrently": "^4.0.1",
    "fs-extra": "^7.0.1",
    "gulp": "^3.9.1",
    "node-sass": "^4.10.0",
    "preact-render-to-string": "^4.1.0",
    "storm-storybook-markup": "^0.1.0"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^4.0.4",
    "@storybook/html": "^4.0.4"
  }
}
